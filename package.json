{
  "name": "electron-vite-vue3-template",
  "version": "0.0.0",
  "main": "./out/main/index.js",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview -c preset/vite/index.*",
    "dev": "electron-vite dev -c preset/vite/index.*",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build -c preset/vite/index.* && electron-builder --win",
    "build:mac": "npm run build -c preset/vite/index.* && electron-builder --mac",
    "build:linux": "npm run build -c preset/vite/index.* && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "electron-updater": "^6.1.8",
    "vue-i18n": "^9.9.1"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify/json": "^2.2.183",
    "@rushstack/eslint-patch": "^1.7.2",
    "@swc/core": "^1.4.1",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "axios": "^1.6.7",
    "electron": "^28.2.3",
    "electron-builder": "^24.13.0",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.10",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.1.3",
    "vite-plugin-vue-devtools": "^7.0.15",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  }
}
